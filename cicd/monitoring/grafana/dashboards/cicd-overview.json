{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "10.0.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "description": "CI/CD Pipeline Overview â€” fitness scores, test results, job durations",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "title": "Pipeline Health",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Fitness Score",
      "description": "Current fitness score from latest pipeline run",
      "type": "gauge",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 40 },
              { "color": "yellow", "value": 60 },
              { "color": "green", "value": 80 }
            ]
          },
          "min": 0,
          "max": 100,
          "unit": "none"
        }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "fitness_score",
          "legendFormat": "{{ project }}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Test Pass Rate",
      "description": "Percentage of tests passing in latest run",
      "type": "gauge",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 80 },
              { "color": "yellow", "value": 90 },
              { "color": "green", "value": 95 }
            ]
          },
          "min": 0,
          "max": 100,
          "unit": "percent"
        }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "cicd_test_pass_rate",
          "legendFormat": "{{ project }}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Total Tests",
      "description": "Total number of tests in latest run",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "cicd_test_total",
          "legendFormat": "Total",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Tests Failed",
      "description": "Number of failed tests",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 }
            ]
          }
        }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "cicd_test_failed",
          "legendFormat": "Failed",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Pipeline Success Rate (24h)",
      "description": "Percentage of successful pipeline runs in the last 24 hours",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 90 }
            ]
          },
          "unit": "percent"
        }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum(rate(cicd_job_status{status=\"success\"}[24h])) / sum(rate(cicd_job_status[24h])) * 100",
          "legendFormat": "Success Rate",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Job Durations",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Job Duration Over Time",
      "description": "Duration of CI/CD jobs over time",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 10 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "s"
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "cicd_job_duration_seconds",
          "legendFormat": "{{ job_name }}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Fitness Score History",
      "description": "Fitness score trend over time",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 10 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 60 },
              { "color": "green", "value": 80 }
            ]
          },
          "custom": {
            "thresholdsStyle": { "mode": "line" }
          }
        }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "fitness_score",
          "legendFormat": "{{ project }}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Gate Results",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Latest Gate Status",
      "description": "Status of each quality gate in the latest pipeline run",
      "type": "table",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 19 },
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "status" },
            "properties": [
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-background",
                  "mode": "basic"
                }
              },
              {
                "id": "mappings",
                "value": [
                  { "options": { "1": { "text": "PASS", "color": "green" } }, "type": "value" },
                  { "options": { "0": { "text": "FAIL", "color": "red" } }, "type": "value" }
                ]
              }
            ]
          }
        ]
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "cicd_job_status",
          "legendFormat": "",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["cicd", "pipeline", "fitness"],
  "templating": {
    "list": [
      {
        "current": {},
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "definition": "label_values(fitness_score, project)",
        "name": "project",
        "label": "Project",
        "type": "query",
        "refresh": 2,
        "includeAll": true,
        "multi": false
      }
    ]
  },
  "time": { "from": "now-24h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "CI/CD Pipeline Overview",
  "uid": "cicd-overview",
  "version": 1
}
